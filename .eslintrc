{
    "parser": "babel-eslint",
    "plugins": [
      "react",
    ],
    "ecmaFeatures": {
      "jsx": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],

    "rules": {
      react/react-in-jsx-scope: 0,
      no-console: "warn",
      comma-dangle: ["error", "only-multiline"]
    },
    "globals": {
      window: true,
      localStorage: true,
      document: true,
      console: true,
      FormData: true,
      alert: true,
      Headers: true,
      fetch: true,
      Promise: true,
      require: true,
      FileReader: true,
      module: true,
      __dirname: true,
    }
}
